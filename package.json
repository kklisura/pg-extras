{
  "name": "pg-extras",
  "version": "0.0.2",
  "description": "CLI tool for various postgres statistics.",
  "author": "Kenan Klisura @kklisura",
  "bin": {
    "pg-extras": "./bin/run"
  },
  "bugs": "https://github.com/kklisura/pg-extras/issues",
  "dependencies": {
    "@oclif/command": "1",
    "@oclif/config": "1",
    "@oclif/plugin-help": "1",
    "@types/moment-duration-format": "^2.2.0",
    "@types/pg": "^7.4.5",
    "ascii-table": "^0.0.9",
    "moment": "^2.22.0",
    "moment-duration-format": "^2.2.2",
    "pg": "^7.4.1",
    "pg-format": "^1.0.4",
    "postgres-interval": "^1.1.1",
    "strip-newlines": "^1.0.1",
    "username": "^3.0.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "1",
    "@oclif/test": "1",
    "@oclif/tslint": "1",
    "@types/chai": "4",
    "@types/mocha": "5",
    "@types/node": "9",
    "chai": "4",
    "globby": "8",
    "mocha": "5",
    "replace": "^0.3.0",
    "ts-node": "5",
    "tslib": "1",
    "tslint": "5",
    "typescript": "2.8"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    ".oclif.manifest.json",
    "/bin",
    "/lib"
  ],
  "homepage": "https://github.com/kklisura/pg-extras",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "pg-extras",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "kklisura/pg-extras",
  "scripts": {
    "postpack": "rm -f .oclif.manifest.json",
    "posttest": "tsc -p test --noEmit && tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc && oclif-dev manifest && oclif-dev readme && ./node_modules/.bin/replace 'kenanklisura' 'user' README.md -s",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
